{
	"name": "greenpeace/planet4-child-theme-brasil",
	"description": "Child theme for the Brasil website of the Planet 4 Wordpress project",
	"type":"wordpress-theme",
	"minimum-stability" : "stable",
	"authors": [
		{
			"name": "Konstantinos Kokkorogiannis",
			"email": "kkokkoro@greenpeace.org"
		}
	],
	"license": "GPL-3.0+",
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org"
		}
	],
	"support": {
		"issues": "https://github.com/greenpeace/planet4-child-theme-brasil/issues",
		"wiki": "https://github.com/greenpeace/planet4-child-theme-brasil/wiki",
		"source": "https://github.com/greenpeace/planet4-child-theme-brasil"
	},
	"extra": {
		"installer-paths": {
			"../../plugins/{$name}/": [
				"wpackagist-plugin/*",
				"type:wordpress-plugin"
			]
		}
	},
	"require": {
		"wpackagist-plugin/bulk-remove-posts-from-category":"3.2.1"
	}
}

